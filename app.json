{
  "expo": {
    "name": "healthSynce",
    "slug": "healthSynce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.healthSynce",
      "infoPlist": {
        "NSHealthShareUsageDescription": "We need access to your health data to sync your activity and workout information.",
        "NSHealthUpdateUsageDescription": "We need access to update your health data with activity and workout details."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.healthSynce"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "react-native-health"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
